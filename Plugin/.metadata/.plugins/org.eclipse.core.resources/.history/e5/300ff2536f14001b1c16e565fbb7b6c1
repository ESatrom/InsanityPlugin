package me.Minecraftmage113.InsanityPlugin.helpers;

import java.util.Map;

import org.bukkit.Material;
import org.bukkit.block.Block;

import com.google.common.collect.ImmutableMap;

public enum InsanityStructures {
	ENDER_GATE,
	ENDER_RIFT,
	ARCANE_ALTAR;
	public Structure create(Block origin, Material... materialVariants) {
		if(materialVariants.length>1) {
			return null;
		}
		Material M1, M2;
		switch(materialVariants.length) {
		case 2:
			M2=materialVariants[1];
		case 1:
			M1=materialVariants[0];
		}
		switch(this) {
		case ARCANE_ALTAR:
			return null;
		case ENDER_GATE:
			return new Structure(1, 1, 0, b, new Material[][][]{
				{{N, N, A},
					 {N, C, N}, 
					 {A, N, N}},
					
					{{},
					 {O, A, O},
					 {T, N, T}},
					
					{{},
					 {O, A, O},
					 {}},
					
					{{O, O, O},
					 {O, O, O},
					 {O, O, O}}});;
		case ENDER_RIFT:
			return null;
		}
		return null;
	}
}
