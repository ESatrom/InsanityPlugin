public class InsanityMetadata extends MetadataValueAdapter {

	String value;
	
	public InsanityMetadata(Plugin owningPlugin) {
		super(owningPlugin);
	}

	@Override
	public Object value() { return value; }

	@Override
	public void invalidate() {
		// Auto-generated method stub
	}

}
